{
  "name": "forestry4-docs",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "documentation for the alpha release of Forestry 4",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:check": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "sync-snippets": "cd ../.. && ./scripts/sync-snippets.sh",
    "build:with-sync": "npm run sync-snippets && npm run build"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/react": "^2.8.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@wonderlandlabs/forestry4": "latest",
    "framer-motion": "^10.16.0",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@types/prismjs": "^1.26.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "jsdom": "^23.0.0",
    "typescript": "^5.0.2",
    "vite": "^7.1.5",
    "vitest": "^3.2.4"
  }
}
