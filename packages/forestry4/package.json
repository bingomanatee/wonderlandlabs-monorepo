{
  "name": "@wonderlandlabs/forestry4",
  "version": "4.1.3",
  "description": "State Management System in Javascript",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">= 20.9 < 21"
  },
  "license": "MIT",
  "dependencies": {
    "@wonderlandlabs/walrus": "0.1.3",
    "immer": "^10.1.1",
    "lodash-es": "^4.17.21",
    "rxjs": "^7.8.1"
  },
  "files": [
    "dist/**/*"
  ],
  "homepage": "https://forestry-4-docs.netlify.app/",
  "devDependencies": {
    "@eslint/js": "~9.10",
    "@types/eslint__js": "~8.42",
    "@types/node": "~20",
    "@typescript-eslint/parser": "~8.5",
    "@vitest/coverage-v8": "~2.1",
    "@vitest/eslint-plugin": "~1.1",
    "eslint": "~9.10",
    "eslint-config-prettier": "~9.1",
    "globals": "~15.9",
    "prettier": "~3.3",
    "rimraf": "~6.0",
    "ts-api-utils": "~1.3",
    "tslib": "~2.7",
    "typescript": "~5.5",
    "typescript-eslint": "~8.5",
    "vite": "~5.4",
    "vite-plugin-dts": "~4.2",
    "vitest": "~2.1",
    "zod": "^4.1.5"
  },
  "scripts": {
    "start": "node dist/index.js",
    "clean": "rimraf coverage dist tmp",
    "prebuild": "npm run clean",
    "build": "vite build",
    "build:watch": "vite build --watch",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage.enabled --coverage.all",
    "prettier": "prettier \"{src,__{tests}__}/**/*.{ts,mts}\" --config .prettierrc --write",
    "prettier:check": "prettier \"{src,__{tests}__}/**/*.{ts,mts}\" --config .prettierrc --check",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "author": "Dave Edelhart <dave@wonderlandlabs.com>",
  "volta": {
    "node": "20.17.0"
  }
}
