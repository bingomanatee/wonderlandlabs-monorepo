{
  "name": "@wonderlandlabs/walrus",
  "version": "0.1.3",
  "description": "utility class for type checking, text ops",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">= 20.9 < 21"
  },
  "license": "MIT",
  "dependencies": {
    "lodash.isequal": "^4.5.0"
  },
  "files": [
    "dist/**/*"
  ],
  "homepage": "https://github.com/bingomanatee/wonderlandlabs-monorepo/tree/main/packages/walrus",
  "devDependencies": {
    "@eslint/js": "~9.10",
    "@types/eslint__js": "~8.42",
    "@types/lodash.isequal": "^4.5.6",
    "@types/node": "~20",
    "@typescript-eslint/parser": "~8.5",
    "@vitest/coverage-v8": "~2.1",
    "@vitest/eslint-plugin": "~1.1",
    "eslint": "~9.10",
    "eslint-config-prettier": "~9.1",
    "globals": "~15.9",
    "prettier": "~3.3",
    "rimraf": "~6.0",
    "ts-api-utils": "~1.3",
    "tslib": "~2.7",
    "typescript": "~5.5",
    "typescript-eslint": "~8.5",
    "vite": "~5.4",
    "vite-plugin-dts": "~4.2",
    "vitest": "~2.1"
  },
  "scripts": {
    "start": "node dist/index.js",
    "clean": "rimraf coverage dist tmp",
    "prebuild": "npm run clean",
    "build": "vite build",
    "build:watch": "vite build --watch",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage.enabled --coverage.all",
    "prettier": "prettier \"{src,__{tests}__}/**/*.{ts,mts}\" --config .prettierrc --write",
    "prettier:check": "prettier \"{src,__{tests}__}/**/*.{ts,mts}\" --config .prettierrc --check",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "type",
    "checking",
    "text",
    "utilities"
  ],
  "author": "Dave Edelhart <dave@wonderlandlabs.com>",
  "volta": {
    "node": "20.17.0"
  }
}
