{
  "transform": {
    "^.+\\.tsx?$": [
      "ts-jest",
      {
        "rootDir": "."
      }
    ] ,
       "^.+\\.ts$": [
      "ts-jest",
      {
        "rootDir": "."
      }
    ]
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(t|j)s$",
  "moduleFileExtensions": ["ts", "js", "json", "node"]
}
