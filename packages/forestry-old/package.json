{
  "name": "@wonderlandlabs/forestry-olf",
  "version": "3.0.10",
  "description": "State Management System in Javascript",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "David Edelhart",
  "license": "MIT",
  "dependencies": {
    "lodash-es": "^4.17.21",
    "rxjs": "^7.8.1"
  },
  "files": [
    "lib/**/*"
  ],
  "homepage": "https://github.com/bingomanatee/wonderlandlabs-monorepo/tree/main/packages/forestry",
  "scripts": {
    "build": "tsc && yarn uglify",
    "uglify": "node uglify.cjs",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest --config jest.config.json",
    "bt": "npm run build; jest --config jest.config.json",
    "prepare": "npm run build; yarn uglify",
    "prepublishOnly": "npm test && yarn lint",
    "preversion": "yarn lint",
    "version": "yarn && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@eslint/js": "^9.9.1",
    "@jest/globals": "^29.7.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.12",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.11",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8",
    "eslint-config-prettier": "^8",
    "eslint-plugin-jest": "^28.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0",
    "uglify-js": "^3.17.4"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  }
}
